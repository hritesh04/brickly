{
  "name": "brickly-monorepo",
  "private": true,
  "version": "0.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "npm@10.7.0",
  "devDependencies": {
    "turbo": "^2.5.8",
    "typescript": "^5",
    "eslint": "^9"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "start": "turbo run start --filter=apps/web..."
  }
}
